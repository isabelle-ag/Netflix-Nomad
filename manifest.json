	"manifest_version": 2,
	"name": "Netflix Nomad",
	"version": "1.0",
  
	"description": "Bypasses household lock for netflix",
  
	"icons": {
	  "48": "./unlock-48.png",
	  "96": "./unlock-96.png"
	},
  
	"content_scripts": [
	  {
		"matches": ["*://*.netflix.com/*"],
